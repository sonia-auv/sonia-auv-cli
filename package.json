{
  "name": "sonia-auv-cli",
  "description": "SONIA AUV CLI Tools",
  "version": "0.0.3",
  "author": "SONIA AUV Team",
  "bin": {
    "sonia": "./bin/run"
  },
  "bugs": "https://github.com/sonia-auv/sonia-auv-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/plugin-help": "^3.1.0",
    "@types/js-yaml": "^3.12.4",
    "@types/listr": "^0.14.2",
    "@types/prompts": "^2.0.8",
    "execa": "^4.0.2",
    "install": "^0.13.0",
    "js-yaml": "^3.14.0",
    "listr": "^0.14.3",
    "lodash": "^4.17.19",
    "npm": "^6.14.7",
    "oclif": "^1.16.1",
    "prompts": "^2.3.2",
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.25",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/sonia-auv/sonia-auv-cli",
  "keywords": [
    "oclif"
  ],
  "license": "GPLv3",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sonia",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ]
  },
  "repository": "sonia-auv/sonia-auv-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc --fix",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
